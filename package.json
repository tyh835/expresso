{
  "name": "expresso",
  "version": "1.0.0",
  "description": "Simple CRUD Express Application with React Frontend",
  "main": "server.js",
  "scripts": {
    "build:client": "cd client && yarn build",
    "install": "yarn install && cd client && yarn install",
    "seed": "node migration.js && node seed.js",
    "start": "NODE_ENV=dev node server.js",
    "start:client": "cd client && yarn start",
    "start:dev": "NODE_ENV=dev nodemon server.js",
    "start:prod": "yarn build:client && NODE_ENV=prod forever start server.js",
    "test": "mocha"
  },
  "author": "Tony Han",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "sqlite3": "^4.0.3",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "forever": "^0.15.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4"
  }
}
